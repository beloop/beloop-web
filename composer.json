{
    "name": "aialahernando/beloop.admin",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "7.2.*",
        "symfony/symfony": "2.8.*",

        "doctrine/annotations": "^1.2",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "^2.2",
        "doctrine/orm": "^2.5",

        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",

        "incenteev/composer-parameter-handler": "^2.0",

        "mmoreram/controller-extra-bundle": "^1.2.1",
        "friendsofsymfony/jsrouting-bundle": "1.6.0",
        "vich/uploader-bundle": "^1.0",

        "beloop/analytics-bundle": "~0.5",
        "beloop/core-bundle": "~0.5",
        "beloop/course-bundle": "~0.5",
        "beloop/language-bundle": "~0.5",
        "beloop/instagram-bundle": "~0.5",
        "beloop/user-bundle": "~0.5",
        "beloop/squarespace-bundle": "~0.5",
        "beloop/typeform-bundle": "~0.5",
        "twig/extensions": "^1.3",
        "liip/imagine-bundle": "^1.5",
        "symfony/assetic-bundle": "^2.8"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "database_host": "DATABASE_HOST",
                "database_port": "DATABASE_PORT",
                "database_user": "DATABASE_USERNAME",
                "database_password": "DATABASE_PASSWORD",
                "database_name": "DATABASE_NAME",
                "analytics_host": "ANALYTICS_HOST",
                "analytics_port": "ANALYTICS_PORT",
                "analytics_user": "ANALYTICS_USERNAME",
                "analytics_password": "ANALYTICS_PASSWORD",
                "analytics_name": "ANALYTICS_NAME"
            }
        }
    }
}
